<html xmlns:o='urn:schemas-microsoft-com:office:office' xmlns:w='urn:schemas-microsoft-com:office:word' xmlns='http://www.w3.org/TR/REC-html40'><head><meta charset='utf-8'><title>Text To Word</title></head><body><p>&lt;!DOCTYPE html&gt;<br>&lt;html lang="en"&gt;<br>&lt;head&gt;<br>&nbsp; &lt;meta charset="UTF-8"&gt;<br>&nbsp; &lt;title&gt;Interactive Feedback Form&lt;/title&gt;<br>&nbsp; &lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt;</p>
<p>&nbsp; &lt;!-- Bootstrap 5 CDN --&gt;<br>&nbsp; &lt;link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"&gt;</p>
<p>&nbsp; &lt;style&gt;<br>&nbsp; &nbsp; body {<br>&nbsp; &nbsp; &nbsp; background: linear-gradient(135deg, #667eea, #764ba2);<br>&nbsp; &nbsp; &nbsp; min-height: 100vh;<br>&nbsp; &nbsp; &nbsp; display: flex;<br>&nbsp; &nbsp; &nbsp; align-items: center;<br>&nbsp; &nbsp; &nbsp; justify-content: center;<br>&nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; .card {<br>&nbsp; &nbsp; &nbsp; border-radius: 20px;<br>&nbsp; &nbsp; &nbsp; animation: slideUp 0.6s ease;<br>&nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; @keyframes slideUp {<br>&nbsp; &nbsp; &nbsp; from { transform: translateY(40px); opacity: 0; }<br>&nbsp; &nbsp; &nbsp; to { transform: translateY(0); opacity: 1; }<br>&nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; .btn-gradient {<br>&nbsp; &nbsp; &nbsp; background: linear-gradient(90deg, #ff7e5f, #feb47b);<br>&nbsp; &nbsp; &nbsp; border: none;<br>&nbsp; &nbsp; &nbsp; color: white;<br>&nbsp; &nbsp; &nbsp; font-weight: bold;<br>&nbsp; &nbsp; &nbsp; transition: transform 0.2s, box-shadow 0.2s;<br>&nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; .btn-gradient:hover:enabled {<br>&nbsp; &nbsp; &nbsp; transform: translateY(-2px);<br>&nbsp; &nbsp; &nbsp; box-shadow: 0 6px 15px rgba(0,0,0,0.3);<br>&nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; .form-control:focus {<br>&nbsp; &nbsp; &nbsp; box-shadow: none;<br>&nbsp; &nbsp; }</p>
<p>&nbsp; &nbsp; .progress {<br>&nbsp; &nbsp; &nbsp; height: 8px;<br>&nbsp; &nbsp; &nbsp; margin-bottom: 20px;<br>&nbsp; &nbsp; }<br>&nbsp; &lt;/style&gt;<br>&lt;/head&gt;<br>&lt;body&gt;</p>
<p>&lt;div class="container"&gt;<br>&nbsp; &lt;div class="row justify-content-center"&gt;<br>&nbsp; &nbsp; &lt;div class="col-md-6 col-lg-5"&gt;<br>&nbsp; &nbsp; &nbsp; &lt;div class="card shadow-lg p-4"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;h3 class="text-center text-primary mb-3"&gt;üåà Feedback Form&lt;/h3&gt;</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &lt;!-- Progress Bar --&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="progress"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;div id="progressBar" class="progress-bar bg-success" style="width: 0%"&gt;&lt;/div&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &lt;form id="feedbackForm" novalidate&gt;</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;!-- Name --&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="mb-3"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;label class="form-label"&gt;Name&lt;/label&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;input type="text" id="name" class="form-control" placeholder="Enter your name" oninput="validateName()"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;small class="text-danger" id="nameError"&gt;&lt;/small&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;!-- Email --&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="mb-3"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;label class="form-label"&gt;Email&lt;/label&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;input type="email" id="email" class="form-control" placeholder="Enter your email" oninput="validateEmail()"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;small class="text-danger" id="emailError"&gt;&lt;/small&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;!-- Message --&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;div class="mb-3"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;label class="form-label"&gt;Message&lt;/label&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;textarea id="message" class="form-control" rows="4" placeholder="Write your feedback" oninput="validateMessage()"&gt;&lt;/textarea&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;small class="text-danger" id="messageError"&gt;&lt;/small&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;/div&gt;</p>
<p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;!-- Submit --&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;button type="button"<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; id="submitBtn"<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; class="btn btn-gradient w-100"<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; ondblclick="submitForm()"<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; disabled&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Submit Feedback ‚≠ê (Double Click)<br>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &lt;/button&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;/form&gt;<br>&nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &lt;/div&gt;<br>&lt;/div&gt;</p>
<p>&lt;!-- Confirmation Modal --&gt;<br>&lt;div class="modal fade" id="confirmationModal" tabindex="-1"&gt;<br>&nbsp; &lt;div class="modal-dialog modal-dialog-centered"&gt;<br>&nbsp; &nbsp; &lt;div class="modal-content rounded-4"&gt;<br>&nbsp; &nbsp; &nbsp; &lt;div class="modal-header bg-primary text-white rounded-top-4"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;h5 class="modal-title"&gt;üéâ Success&lt;/h5&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal"&gt;&lt;/button&gt;<br>&nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &nbsp; &lt;div class="modal-body text-center fs-5"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;p id="confirmationText"&gt;&lt;/p&gt;<br>&nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &nbsp; &lt;div class="modal-footer justify-content-center"&gt;<br>&nbsp; &nbsp; &nbsp; &nbsp; &lt;button class="btn btn-primary" data-bs-dismiss="modal"&gt;Close&lt;/button&gt;<br>&nbsp; &nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &nbsp; &lt;/div&gt;<br>&nbsp; &lt;/div&gt;<br>&lt;/div&gt;</p>
<p>&lt;!-- Bootstrap JS --&gt;<br>&lt;script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"&gt;&lt;/script&gt;</p>
<p>&lt;script&gt;<br>&nbsp; const nameField = document.getElementById("name");<br>&nbsp; const emailField = document.getElementById("email");<br>&nbsp; const messageField = document.getElementById("message");<br>&nbsp; const submitBtn = document.getElementById("submitBtn");<br>&nbsp; const progressBar = document.getElementById("progressBar");</p>
<p>&nbsp; function validateName() {<br>&nbsp; &nbsp; const name = nameField.value.trim();<br>&nbsp; &nbsp; if (!/^[A-Za-z\s]{2,}$/.test(name)) {<br>&nbsp; &nbsp; &nbsp; setInvalid(nameField, "nameError", "Enter at least 2 letters");<br>&nbsp; &nbsp; } else {<br>&nbsp; &nbsp; &nbsp; setValid(nameField, "nameError");<br>&nbsp; &nbsp; }<br>&nbsp; &nbsp; updateProgress();<br>&nbsp; }</p>
<p>&nbsp; function validateEmail() {<br>&nbsp; &nbsp; const email = emailField.value.trim();<br>&nbsp; &nbsp; const pattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;<br>&nbsp; &nbsp; if (!pattern.test(email)) {<br>&nbsp; &nbsp; &nbsp; setInvalid(emailField, "emailError", "Invalid email address");<br>&nbsp; &nbsp; } else {<br>&nbsp; &nbsp; &nbsp; setValid(emailField, "emailError");<br>&nbsp; &nbsp; }<br>&nbsp; &nbsp; updateProgress();<br>&nbsp; }</p>
<p>&nbsp; function validateMessage() {<br>&nbsp; &nbsp; const message = messageField.value.trim();<br>&nbsp; &nbsp; if (message.length &lt; 5) {<br>&nbsp; &nbsp; &nbsp; setInvalid(messageField, "messageError", "Minimum 5 characters required");<br>&nbsp; &nbsp; } else {<br>&nbsp; &nbsp; &nbsp; setValid(messageField, "messageError");<br>&nbsp; &nbsp; }<br>&nbsp; &nbsp; updateProgress();<br>&nbsp; }</p>
<p>&nbsp; function setInvalid(field, errorId, msg) {<br>&nbsp; &nbsp; field.classList.add("is-invalid");<br>&nbsp; &nbsp; field.classList.remove("is-valid");<br>&nbsp; &nbsp; document.getElementById(errorId).textContent = msg;<br>&nbsp; }</p>
<p>&nbsp; function setValid(field, errorId) {<br>&nbsp; &nbsp; field.classList.add("is-valid");<br>&nbsp; &nbsp; field.classList.remove("is-invalid");<br>&nbsp; &nbsp; document.getElementById(errorId).textContent = "";<br>&nbsp; }</p>
<p>&nbsp; function updateProgress() {<br>&nbsp; &nbsp; let validCount = 0;<br>&nbsp; &nbsp; if (nameField.classList.contains("is-valid")) validCount++;<br>&nbsp; &nbsp; if (emailField.classList.contains("is-valid")) validCount++;<br>&nbsp; &nbsp; if (messageField.classList.contains("is-valid")) validCount++;</p>
<p>&nbsp; &nbsp; const percent = (validCount / 3) * 100;<br>&nbsp; &nbsp; progressBar.style.width = percent + "%";</p>
<p>&nbsp; &nbsp; submitBtn.disabled = validCount !== 3;<br>&nbsp; }</p>
<p>&nbsp; function submitForm() {<br>&nbsp; &nbsp; const modal = new bootstrap.Modal(document.getElementById("confirmationModal"));<br>&nbsp; &nbsp; document.getElementById("confirmationText").textContent =<br>&nbsp; &nbsp; &nbsp; `Thank you, ${nameField.value}! Your feedback has been submitted successfully üíñ`;</p>
<p>&nbsp; &nbsp; modal.show();</p>
<p>&nbsp; &nbsp; document.getElementById("feedbackForm").reset();<br>&nbsp; &nbsp; [nameField, emailField, messageField].forEach(f =&gt; {<br>&nbsp; &nbsp; &nbsp; f.classList.remove("is-valid", "is-invalid");<br>&nbsp; &nbsp; });</p>
<p>&nbsp; &nbsp; progressBar.style.width = "0%";<br>&nbsp; &nbsp; submitBtn.disabled = true;<br>&nbsp; }<br>&lt;/script&gt;</p>
<p>&lt;/body&gt;<br>&lt;/html&gt;</p></body></html>